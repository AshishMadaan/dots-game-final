olid #bbbbbb;\n  border-radius: 3px;\n  padding: 6px 18px;\n  margin: 12px;\n  color: #303030;\n  letter-spacing: 0.1em;\n  width: 80px;\n}\n\nfooter {\n  position: absolute;\n  display: flex;\n  align-items: center;\n  justify-content: space-between;\n  padding: 16px 32px;\n  box-sizing: border-box;\n  width: 100vw;\n  bottom: 0;\n  user-select: none;\n  overflow: hidden;\n\n  .game__score {\n    font-size: 1.5em;\n    font-weight: 600;\n  }\n\n  .game__toggle .pause__button {\n    display: flex;\n    position: relative;\n    border: none;\n    outline: none;\n    border-radius: 10px;\n    padding: 1rem 2rem;\n    margin: 0 auto;\n    background: #1778b9;\n    color: #fff;\n    font-size: 1rem;\n    cursor: pointer;\n    text-align: center;\n  }\n\n  .game__slider {\n    position: relative;\n    display: flex;\n    align-items: center;\n\n    .slider-speed-text {\n      display: inline-block;\n      margin-right: 20px;\n      min-width: 80px;\n    }\n\n    .slider__speed {\n      display: inline-block;\n      min-width: 50px;\n      text-align: right;\n    }\n  }\n\n}\n\n.timer {\n  visibility: hidden;\n}\n\n.mode__selection{\n  visibility: hidden;\n  align-items: center;\n  display: flex;\n  justify-content: center;\n}\n\n.show{\n  visibility: visible;\n}\n\n\n@media screen and (max-width: $tablet-width) {\n  header .game__title {\n    display: none;\n  }\n  .reset__button.btn {\n    display: none;\n  }\n  main .game__board {\n    top: 48px;\n    height: $tablet-height;;\n  }\n  .score__text {\n    display: none;\n  }\n  .mode__selection.show{\n    visibility: hidden;\n  }\n  .timer.show{\n    visibility: hidden;\n  }\n  footer {\n    top: 0;\n    bottom: unset;\n    overflow: visible;\n\n    input {\n      width: 80%;\n    }\n\n    .game__toggle {\n      right: 32px;\n      width: unset;\n    }\n\n    .game-score .score__text {\n      display: none;\n    }\n\n    .game__slider {\n      position: absolute;\n      top: 64px;\n\n      .slider__speed {\n        visibility: visible;\n        width: unset;\n        min-width: 50px;\n      }\n\n      .slider-speed-text {\n        display: block;\n        position: absolute;\n        border: 0;\n        top: 30px;\n      }\n\n      .slider-handle {\n        left: 0;\n        width: 85vw;\n      }\n    }\n  }\n}\n\n","styles/variables.scss":"$tablet-width: 768px;\n$desktop-width: 1024px;\n\n\n$tablet-height: 80vh;\n$desktop-height: 500px;\n","styles/overlay.scss":".modal__overlay {\n  background-color: rgba(0, 0, 0, 0.9);\n  color: #171016;\n  display: none;\n  height: 100vh;\n  position: fixed;\n  width: 100%;\n  z-index: 1;\n\n  &.active {\n    display: block;\n    top: 0;\n    bottom: 0;\n    left: 0;\n    right: 0;\n    margin: auto;\n  }\n\n  button {\n    margin: 2.5em;\n    min-width: 30px;\n    position: absolute;\n    right: 0;\n    top: 0;\n    width: 0;\n  }\n\n  .modal__content {\n    background-color: #b8b3e9;\n    border-radius: 10px;\n    font-size: 1.5em;\n    left: 50%;\n    margin: 0;\n    padding: 2.5em;\n    position: absolute;\n    top: 50%;\n    -ms-transform: translate(-50%, -50%);\n    transform: translate(-50%, -50%);\n  }\n}\n\n\n.close.btn {\n  position: absolute;\n  right: 0;\n  background-color: cornflowerblue;\n  padding: 7px 10px;\n  width: auto;\n  margin: 0.5rem;\n}","styles/animation.scss":"@keyframes slideDown {\n  0% {\n    transform: translateY(0);\n  }\n\n  100% {\n    transform: translateY($desktop-height);\n  }\n}\n\n@media screen and (max-width: $tablet-width) {\n  @keyframes slideDown {\n    0% {\n      transform: translateY(0);\n    }\n  \n    100% {\n      transform: translateY($tablet-height);\n    }\n  }\n}"},"lineCount":null}},"error":null,"hash":"dce593a59dbbf728c83c0a957f93c793","cacheData":{"env":{}}}